    "p = 97\n",
    "q = 37\n",
    "PT = 22\n",
    "n = p*q\n",
    "phiN = (p-1)*(q-1)\n",
    "\n",
    "e = 17\n",
    "# print(phiN)\n",
    "d = 0\n",
    "for i in range(0,99):\n",
    "    d = ((phiN * i) + 1)/e\n",
    "    if d.is_integer():\n",
    "        break\n",
    "\n",
    "CT = (PT**e)%n\n",
    "print(CT)\n",
    "\n",
    "PT = (CT**int(d))%n\n",
    "print(PT)\n",
    "\n"
